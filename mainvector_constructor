using namespace std;
void Vector::distancia(){
        float a;
        a=pow(start.getx()-end.getx(),2)+pow(start.gety()-end.gety(),2);
        a=sqrt(a);
        cout<<a<<endl;
}
 void punto::impr(){
    cout<<"la coordenada del punto x: "<<getx()<<endl;
    cout<<"la coordenada del punto y: "<<gety()<<endl;
}






